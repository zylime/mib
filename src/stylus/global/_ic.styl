  &--reservation
    background url(../img/ic-resv.png)
    background-size 100%
    background-repeat no-repeat
    background-position center
    width 34px
    height 34px
  &--back
    background-image url(../img/ic-left.png)
    background-repeat no-repeat
    background-size 100%
    display block
    width 7px
    height 25px
    background-position center
